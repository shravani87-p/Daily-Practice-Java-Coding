package basicArrayQuePractice;

public class ProductOfPrimeNum {
public static void main(String[] args) {
	int a[]= {1,2,3,7,5};
	int prod=1;
	for(int ele: a)
	{
		if(isPrime(ele))
		{
			prod=prod*ele;
		}
		
	}
	System.out.print("Product of prime num is: "+prod);
}
	public static boolean isPrime(int a)
	{
		int count=0;
		if(a<=1)
			return false;
		for(int i=1;i<=a;i++)
		{
			if(a%i==0)
			{
				count++;
			}
		}
		return count==2;
	}
}

