package array;

public class AvgOfDifferentAge {
public static void main(String[] args) {
	int age[]= {1,26,81,90};
	int avg,sum=0;
	int count=0;
	for(int i=0;i<age.length;i++)
	{
		count++;
		sum=sum+age[i];
	}
	System.out.println("Sum of age is: "+sum);
	avg=sum/count;
	System.out.println("Avg of age is : "+avg);
}
}
