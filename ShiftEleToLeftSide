package array;

import java.util.Arrays;

public class ShiftEleToLeftSide {
public static void main(String [] args)
{
	//        20 30 40 50 10
	int a[]= {10,20,30,40,50};
	System.out.println(Arrays.toString(leftShift(a)));
	
}
public static int[] leftShift(int a[])
{
	int temp=a[0];
	for(int i=0;i<a.length-1;i++)
	{
		a[i]=a[i+1];
		
	}
	a[a.length-1]=temp;
	return a;
}

}
