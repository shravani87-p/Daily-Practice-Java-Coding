package array;

public class CommonEleBetweenTwoArray {
public static void main(String[] args) {
	int a[]= {1,2,3,4,5,6};
	int b[]= {6,5,7,8,9};
	commonEle(a,b);
}
public static void commonEle(int a[],int b[])
{
	int count=0;
	for(int i=0;i<a.length;i++)
	{
		for(int j=0;j<b.length;j++)
		{
			if(a[i]==b[j])
			{
				
				count++;
			}
		}
	}
	System.out.println(" total Count of common Element is: "+count);
	
}
}
