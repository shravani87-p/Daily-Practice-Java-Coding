package array;

public class FrequencyOfElement {
public static void main(String[] args) {
	int a[]= {1,2,4,5,6,2};
	freqEle(a);
}
public static void freqEle(int a[])
{
	boolean visited[]=new boolean[a.length];
	for(int i=0;i<a.length;i++)
	{
		if(visited[i])
			continue;
		
		int count=1;
		for(int j=i+1;j<a.length;j++)
		{
			if(a[i]==a[j])
			{
				visited[j]=true;
			count++;
			}
		}
		if(count>1)
		{
			System.out.println(a[i]+"Frequency of ele: "+count);
		}
	}
}
}
