package array;

import java.util.Arrays;

public class NthLeftShift {
public static void main(String args [])
{
	int n=4;
		//    5 6 7 8 1 2 3 4
	int a[]= {1,2,3,4,5,6,7,8};
	System.out.println(Arrays.toString(nthLeft(a,n)));
}
public static int[] nthLeft(int a[], int n)
{
	for(int k=1;k<=n;k++)
	{
		int temp=a[0];
		for(int i=0;i<a.length-1;i++)
		{
			a[i]=a[i+1];
		}
		a[a.length-1]=temp;
	}
	return a;
}
}
