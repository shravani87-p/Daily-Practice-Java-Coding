package array;

public class LinearSearchAlgorithm {
public static void main(String[] args) {
	int a[]= {10,20,30,60,50};
	System.out.println(searchEle(a, 60, 0));
}
public static int searchEle(int a[], int ele, int index)
{
	if(index==a.length)
		return -1;
	
	if(a[index]==ele)
		return index;
	
	return searchEle(a,ele,index+1);
}
}
