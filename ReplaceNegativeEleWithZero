package array;

import java.util.Arrays;

public class ReplaceNegativeEleWithZero {
public static void main(String[] args) {
	int a[]= {1,-2,6,90,-1};
	System.out.println(Arrays.toString(replaceNegToZero(a)));
}
public static int[] replaceNegToZero(int a[])
{
	for(int i=0;i<a.length;i++)
	{
		if(a[i]<0)
		{
			a[i]=0;
		}
	}
	return a;
}
}
