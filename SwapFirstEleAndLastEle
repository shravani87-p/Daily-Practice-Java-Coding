package array;

import java.util.Arrays;

public class SwapFirstEleAndLastEle {
public static void main(String[] args) {
	int a[]= {1,2,3,4,5};
	System.out.println(Arrays.toString(swapEle(a)));
}
public static int[] swapEle(int a[])
{
	int temp=a[0];
	a[0]=a[a.length-1];
	a[a.length-1]=temp;
	return a;
}
}
