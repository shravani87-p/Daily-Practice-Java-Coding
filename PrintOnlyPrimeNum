package array;

import java.util.Arrays;

public class PrintOnlyPrimeNum {
	public static void main(String[] args) {
		int a[]= {10,11,12,3,4,5,6,7};
		for(int ele:a)
		{
			if(isPrime(ele))
			{
				System.out.println("Prime element is: "+ele);
			}
		}
	}
	public static boolean isPrime(int a)
	{
		int count=0;
		if(a<=1)
		{
			return false;
		}
		for(int i=1;i<=a;i++)
		{
			if(a%i==0)
			{
				count++;
			}
		
		}
		return count==2;
	}
}
