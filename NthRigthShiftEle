package array;

import java.util.Arrays;

public class NthRigthShiftEle {
public static void main(String [] args)
{
		//    7 8 9 1 2 3 4 5 6
	int a[]= {1,2,3,4,5,6,7,8,9};
	int n=3;
	System.out.println(Arrays.toString(nthRight(a,n)));
}
public static int[] nthRight(int a[],int n)
{
	for(int k=1;k<=n;k++)
	{
		int temp=a[a.length-1];
		for(int i=a.length-1 ;i>=1;i--)
		{
			a[i]=a[i-1];
		}
		a[0]=temp;
	
	}
	return a;
}
}
