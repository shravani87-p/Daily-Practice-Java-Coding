package basicArrayQuePractice;

public class SumOfPrimeEle {
	
	public static void main(String[] args) {
		int a[]= {1,6,31,6,7,3,5};
		int sum=0;
		for(int ele:a)
		{
			if(isPrime(ele))
				{
					sum=sum+ele;
				}
			
		}
		System.out.println("Sum is: "+sum);
	}
	
	public static boolean isPrime(int a)
	{
		int count=0;
//		if(a<=1)
//		{
//			return false;
//		}
		for(int i=1;i<=a;i++)
		{
			if(a%i==0)
			{
				count++;
			}
		}
		return count==2; 
		
	}

}
