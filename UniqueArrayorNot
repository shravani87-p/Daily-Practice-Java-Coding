package array;

public class UniqueArrayorNot {
public static void main(String[] args) {
	int a[]= {1,2,3,4};
	System.out.println(isUnique(a));
}
public static boolean isUnique(int a[])
{
	for(int i=0;i<a.length;i++)
	{
		for(int j=i+1;j<a.length;j++)
		{
			if(a[i]==a[j])
			{
				return false;
			}
		}
	}
	return true;
}
}
