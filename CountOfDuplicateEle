public class CountOfDuplicateEle {
public static void main(String[] args) {
	int [] a= {1,3,2,4,1,2,3};
//	for(int num:a)
//	{
//		if(isDuplicate(num))
//		{
//			System.out.println("Count of duplicate variable: "+count);
//		}
//	}
	System.out.println(isDuplicate(a));
	
}

public static int isDuplicate(int a[])
{
	int count=0;
	for(int i=0;i<a.length;i++)
	{
		for(int j=i+1;j<a.length;j++)
		{
			if(a[i]==a[j])
			{
				count++;
				break;
			}
		}
	}
	return count;
}
}
