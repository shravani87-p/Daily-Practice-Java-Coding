package array;

import java.util.Arrays;

public class StoredAllMissingElementsInsideAnNewArray {
public static void main(String[] args) {
	int a[]= {1,4,3,6,7};
	System.out.println(Arrays.toString(missingEle(a)));
}
public static int[] missingEle(int a[])
{
	int max=max(a);
	int res[]=new int[max];
	int index=0;
	
	boolean present[]=new boolean[max+1];
	for(int num:a)
	{
		present[num]=true;
	}
	for(int i=1;i<=max;i++)
	{
		if(!present[i])
		{

			res[index]=i;
			index++;
		}
	}
//	return res;
	int output[]=new int[index];
	for(int i=0;i<index;i++)
	{
		if(res[i]!=0)
		{
			output[i]=res[i];
		}
	}
	return output;
	
}
public static int max(int a[])
{
	int max=a[0];
for(int i=1;i<a.length;i++)
{
	if(a[i]>max)
		max=a[i];
}
//System.out.println("Max value is: "+max);
return max;

}
}
